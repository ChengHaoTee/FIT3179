{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title":"Top 10 Countries Score",
    "width":800,
    "data":{"url": "https://raw.githubusercontent.com/ChengHaoTee/FIT3179/main/Assignment/Assignment_Data/Score_Life_Freedom.csv"},
    "hconcat": [{
        "selection": {
          "brush": {"type": "interval"}
        },
        "mark": "point",
        "encoding": {
          "x": {"field": "Ladder score","type": "quantitative","scale":{"domain":[0,10]}},
          "y": {"field": "Healthy life expectancy","type": "quantitative"}
        }
      }, {
        "transform": [
          {"filter": {"selection": "brush"}}
        ],
        "mark": "point",
        "encoding": {
          "x": {
            "field": "Ladder score", "type": "quantitative",
            "scale": {"domain": [0,10]}
          },
          "y": {
            "field": "Freedom to make life choices","type": "quantitative",
            "scale": {"domain": [0, 1]}
          }
        }
      }]
}
