{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "height":400,
    "width": 800,
    "title": {
      "text": "World Happiness Score Rank",
      "frame": "bounds"
    },
    "data": {
        "url": "https://raw.githubusercontent.com/ChengHaoTee/FIT3179/main/Assignment/Assignment_Data/RankOverTime.csv"
    },
    "transform": [{
      "sort": [
        {"field": "Happiness Rank", "order": "ascending"}
      ],
      "window": [{
        "op": "rank",
        "as": "rank"
      }],
      "groupby": ["Year"]
    }],
    "mark": {"type": "line", "orient": "vertical"},
    "encoding": {
      "x": {"field": "Year", "type": "ordinal"},
      "y": {"field": "rank", "type": "ordinal"},
      "color": {
        "field": "Country", "type": "nominal",
        "scale": {
          "domain": ["Switzerland", "Iceland", "Denmark", "Sweden","Norway","Canada","Finland","Netherlands","New Zealand","Australia"],
          "range": ["black", "blue", "yellow", "green","red","pink","purple","darkblue","gray","orange"]
        }
      }
    }
  }